<script>
function fullscreen(){
	if(document.getElementById('video1').requestFullscreen){
		document.getElementById('video1').requestFullscreen();
	}else if(document.getElementById('video1').mozRequestFullscreen){
		document.getElementById('video1').mozRequestFullscreen();
	}else if(document.getElementById('video1').webkitRequestFullscreen){
		document.getElementById('video1').webkitRequestFullscreen();
	}else if(document.getElementById('video1').msRequestFullscreen){
		document.getElementById('video1').msRequestFullscreen();
	}

}

</script>
</head>
<body>
<div style="width: 300px;">
<video id="video1" controls="fullscreen" style="width: 100%">
<source src="01.mp4">
</video>
</div>
<div style="width:100px;height:30px;"><a href="#" onclick="fullscreen(); return false;">전체화면</a></div>
</body>
</html>
