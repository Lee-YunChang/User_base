-webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}
.link2 ul > li a:before {content:''; display:block; margin:0 auto; padding:12px 0 5px; width:47px; height:47px; background:url('/academy/images/main/icon10.png') no-repeat; background-position:0px 10px;}
.link2 ul > li:first-child a:before {content:''; display:block; margin:0 auto; padding:12px 0 5px; width:47px; height:47px; background:url('/academy/images/main/icon9.png') no-repeat; background-position:0px 10px;}

/* sns */
.sns-wrap {float:left; width:37.5%; padding-left:10px;
	-webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}

.sns {height:97px; border:1px solid #e4e4e4; border-radius:8px; padding:0 22px; box-sizing:border-box; text-align:center;
	-webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}
.sns ul {overflow:hidden;}
.sns ul li {float:left;	width:25%;}
.sns ul li a{display:block; font-family:'ng_bold'; color:#999;}
.sns ul li a:before {content:''; display:block; margin:12px auto 4px; width:38px; height:38px; background:url('/academy/images/main/sns1.png') no-repeat; background-size:100%;}
.sns ul li a.kakao:before {background:url('/academy/images/main/sns2.png') no-repeat;}
.sns ul li a.blog:before {background:url('/academy/images/main/sns3.png') no-repeat;}
.sns ul li a.w-zine:before {background:url('/academy/images/main/sns4.png') no-repeat;}
.sns > h2 {display:none;}


/* 통합검색 */
.serch-wrap {float:right; width:37.5%; padding-left:10px;
-webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;
}
.serch {height:97px; border:none; border-radius:8px; background:#f7921e; padding:28px 30px 19px; box-sizing: border-box;
 -webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;
 }
.serch h2 {font-size:1.308em; line-height:0px; font-family:'ng_bold'; color: #fff;}
.serch h2 > label {display:block; margin-bottom:9px;}
.serch input[type="text"] {position:absolute; left:0; width:100%; text-indent:10px; font-size:0.923em; color:#848484;}
.serch input[type="submit"] {position:absolute; right:0; display:inline-block; width:30px;  background:url('/academy/images/main/icon11.png') no-repeat; background-position:0px 5px; cursor:pointer;}
.serch input[type="text"],
.serch input[type="submit"] {border:0; height:30px;}
.serch input[type="submit"] {text-indent:-9999px;}
.serch div {position:relative; height:30px; margin:20px 0px 20px; background:#fff;}

.wrap {overflow:hidden; position:relative; width:100%;}
/* FOOTER */
.footer-wrap{border-top:1px solid #e4e4e4; margin-top:70px; clear:both;}
#footer {background:#787878;}

.f-menu {background:#fff;}
.f-menu > ul {overflow:hidden; width:1000px; margin:0 auto;}
.f-menu > ul li {float:left; width:152px;}
.f-menu > ul li > a {position:relative; display:block; height:40px; line-height:40px; text-align:center; font-size:0.923em; color:#848484; font-family:'돋움', Dotum;} 
.f-menu > ul li > a:after {content:''; display:block; position:absolute; top:0; right:0; width:1px; height:100%; background:#e4e4e4;}
.f-menu > ul li:first-child > a {border-left:1px solid #e4e4e4;}

.tell-info > h2 {font-size:1.308em; padding-left:27px; font-family:'ng_bold'; line-height:22px; color:#84e1ff; background:url('/academy/images/main/icon12.png') no-repeat;}
.tell-info > ul {overflow:hidden; padding-left:21px;}
.tell-info > ul:after {display:block; content:""; clear:both;}
.tell-info > ul li {position:relative; float:left; padding:0 6px 0; line-height:24px; color:#fff;}
.tell-info > ul li.on {background:#9f9f9f; border-radius:2px; color:#fff;}
.tell-info > ul li:before {position:absolute; top:50%; left:0; content:''; display:inline-block; width:2px; height:1px; background:#fff;}
/* .tell-info > ul li:first-child:before, */
.tell-info > ul li.on:before{background:none;}
#footer .adress {margin-bottom:5px;}
#footer .adress,
#footer .copyright {font-size:0.923em; line-height:14px; color:#fff;}

.footer-top {border-bottom:1px solid #9d9d9d; padding:18px 0 12px;}
.footer-bottom {width:1000px;height:67px;margin:0 auto; font-family:'돋움';padding:20px 0px 20px;}
.footer-bottom div {float:left;}
.footer-bottom .WA-mark {float:left;width:90px;margin-left: 50px;}

.tell-info,
.footer-btm {width:1000px; margin:0 auto; position:relative;}
.footer-btm {padding:20px 0;}
.left-area {display:none; position:absolute; top:50%; left:20px; margin-top:-16px;}
.tell-info{padding:0 40px; line-height:32px;}
.footer-btm {padding:20px 0px 20px; box-sizing: border-box; font-family:'돋움';
-webkit-box-sizing:border-box; -moz-box-sizing:border-box;}
.footer-btm .abs.web {top:14px; right:2px; text-indent:-9999px; background:url('/academy/images/main/web_icon.png') no-repeat; width:88px; height:62px;}


/* mobile gnb */
.gnb-layer {display:none; overflow-y:auto;position:absolute;left:-480px;top:0;bottom:0;z-index:100;width:480px;background:#fff;}
.gnb-menu > h2 > a.m_close {position:relative; line-height:98px; padding:0 40px; display:block; font-size:1.846em; font-family:'ng_bold'; color:#00aee7; border-bottom:1px solid #c0c0c0;}
.gnb-menu > ul > li {}
.gnb-menu > ul > li > a {position:relative; display:block; padding:0 40px; font-size:1.846em; height:98px; line-height:98px; font-family:'ng_bold'; color:#454545; border-bottom:1px solid #c0c0c0;}
.gnb-menu > ul > li.on > a{color:#fff; background:#00aee7;}
.gnb-menu > ul > li.on > a:after {content:''; display:inline-block; position:absolute; top:50%; right:40px; margin-top:-14px; width:34px; height:34px; background:url('/academy/images/main/m/open.png') no-repeat; background-size:100%;}
.gnb-menu > h2 > a.m_close:after{content:''; display:inline-block; position:absolute; top:50%; right:40px; margin-top:-14px; width:28px; height:28px; background:url('/academy/images/main/m/m_close.png') no-repeat; background-size:100%;} 
.gnb-menu > ul > li > a:after {content:''; display:inline-block; position:absolute; top:50%; right:40px; margin-top:-18px; width:36px; height:36px; background:url('/academy/images/main/m/close.png') no-repeat; background-size:100%;} 

.m-inner {display: none;}
.m-inner > li {border-bottom:1px solid #d0d0d0;}
.m-inner > li a {position:relative; color:#454545; display:block; padding:0 60px; line-height:82px; height:82px; font-size:1.692em; background:#f5f5f5;}
.m-inner > li a:before {content:''; display:inline-block; position:absolute; top:50%; left:40px; margin-top:-4px; width:8px; height:8px; background:url('/academy/images/main/m/m_dot.png') no-repeat; background-size:100%;} 


.bg-modal {display:none;}

